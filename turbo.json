{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "storybook": {
      "dependsOn": [
        "^storybook"
      ]
    },
    "build:storybook": {
      "dependsOn": [
        "^build:storybook"
      ],
      "outputs": [
        "storybook-static/**"
      ]
    }
  }
}
